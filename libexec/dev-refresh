#!/usr/bin/env bash

rbenv update
pyenv update
nodenv update

eval "$(rbenv init -)"

rbenv shell --unset

brew upgrade
brew cask upgrade

rbenv global system
rbenv uninstall -f 2.8.0-dev
rbenv install 2.8.0-dev
rbenv global 2.8.0-dev

rbenv shell 2.5
gem update

rbenv shell 2.5
dev all-pristine
rbenv shell 2.6
dev all-pristine
rbenv shell 2.7
dev all-pristine
rbenv shell 2.8
dev all-pristine

dev gem-shebang-update

rbenv shell --unset
