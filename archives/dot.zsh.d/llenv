# rbenv
export PATH=$HOME/.rbenv/bin:$PATH
eval "$(rbenv init -)"
alias -g rb='rbenv'

# ndenv
export PATH=$HOME/.ndenv/bin:$PATH
eval "$(ndenv init -)"
alias -g nd='ndenv'

# plenv
export PATH=$HOME/.plenv/bin/:$PATH
eval "$(plenv init -)"
alias -g pl='plenv'

# phpenv
export PATH=$HOME/.phpenv/bin:$PATH
eval "$(phpenv init -)"
alias -g ph='phpenv'

# pyenv
export PATH=$HOME/.pyenv/bin:$PATH
eval "$(pyenv init -)"
alias -g py='pyenv'
